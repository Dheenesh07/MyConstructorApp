{
  "expo": {
    "name": "Beemji Builders",
    "slug": "beemji-builders",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/beemji logo ai.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/beemji logo ai.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/beemji logo ai.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.kumaravel07.beemjibuilders",
      "softwareKeyboardLayoutMode": "pan",
      "permissions": []
    },
    "web": {
      "favicon": "./assets/beemji logo ai.png"
    },
    "extra": {
      "eas": {
        "projectId": "9c476546-3b69-45f3-b863-e67fe13d6774"
      }
    },
    "owner": "kumaravel_07",
    "plugins": [
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ]
  }
}
